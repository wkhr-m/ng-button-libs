{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "wm",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "wm",
      "kebab-case"
    ],
    "no-input-rename": false,
    "no-inferrable-types": false,
    "no-non-null-assertion": false
  }
}